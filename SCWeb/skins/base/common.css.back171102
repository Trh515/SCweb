    }

        .batch_act .z_select .tri_up {
            background: url(../default/images/main_sprit1.png) no-repeat -38px -575px;
        }

.batch_dropup {
    display: none;
    position: absolute;
    left: 0;
    bottom: 26px;
    z-index: 100;
    width: 100%;
    border: 1px solid #dcdcdc;
    border-width: 1px 1px 0 1px;
    box-sizing: border-box;
    background: #fff;
}

    .batch_dropup li {
        padding: 0 3px;
        height: 24px;
        line-height: 24px;
        border-bottom: 1px dashed #dcdcdc;
        cursor: pointer;
    }

        .batch_dropup li a {
            display: block;
        }

        .batch_dropup li:hover {
            background: #f6f6f6;
        }

        .batch_dropup li:last-child, .batch_dropup li.nobd {
            border-bottom: none;
        }

