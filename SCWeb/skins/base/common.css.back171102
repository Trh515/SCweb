    border: 1px solid #dcdcdc;
    border-width: 0 1px;
}

    .page_drop li {
        text-align: center;
        border-bottom: 1px solid #dcdcdc;
        cursor: pointer;
    }

        .page_drop li:hover {
            color: #f08300;
        }

.pageDown_btn {
    margin-left: 10px;
    height: 24px;
    border: 1px solid #dcdcdc;
    border-right: none;
    background: #fff;
}

    .pageDown_btn span {
        float: left;
