                    </div>

                    <div class="f_cb f_mt25 spxg">

                        <label class="add_label f_fl">
                            <i class="red_point">*</i>红皮资料日期
                        </label>
                        <div class="f_fl xg1">
                            <div class="f_cb controls_calendar">
                                <input type="text" class="Wdate" id="SCJD92" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd',readOnly:true})" value="@ViewBag.SCJD92" />
                            </div>
                        </div>
                    </div>

                    <!--按钮-->
                    <div class="spxg bf_bnt">
                        <div class="add_r f_fl">
                            <!-- 保存并返回 -->
                            <div class="view_btn f_mt30">
                                <div class="sp_deletbtn baocun bjym" onclick="javascript: window.history.back(-1);">
                                    <span class="bnt_span">
                                        <img class="bnt_img" src="/skins/img/back.png">
                                    </span>返回
                                </div>
                                <div id="btnBC" class="sp_xjbtn baocun bjym">
                                    <span class="bnt_span" id="btnKeep"><img class="bnt_img" src="/skins/img/ico04.png" style=""></span>保存
                                </div>

                            </div>
                          


                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <script type="text/javascript">
        $(function () {
            var id = "@ViewBag.id";

            //保存编辑
            $("#btnBC").click(function () {
                $.ajax({
                    url: "/GoodsInfo/GetEditQACheckSCJDB",
                    timeout: 0, //超时时间设置，单位毫秒
                    type: "post",
                    data:
                        {
                            id: id,
                            SCJD05: $("#SCJD05").val(),
                            SCJD31: $("#SCJD31").val(),
                            SCJD58: $("#SCJD58").val(),
                            SCJD49: $("#SCJD49").val(),
                            SCJD59: $("#SCJD59").val(),
                            SCJD60: $("#SCJD60").val(),
                            SCJD61: $("#SCJD61").val(),
                            SCJD62: $("#SCJD62").val(),
                            SCJD63: $("#SCJD63").val(),
                            SCJD64: $("#SCJD64").val(),
                            SCJD65: $("#SCJD65").val(),
                            SCJD92: $("#SCJD92").val()
                        },
                    success: function (data) {
                        
                        if (data == "OK") {
                            layer.open({
                                title: '消息',
                                content: '编辑成功！',
                                btn: ['确定'],
                                yes: function (index, layero) {
                                    window.history.back(-1);
                                }
                            });
                        } else if (data == "Operation") {
                            layer.alert("您没有权限请和管理员联系");
                        }
                        else {
                            layer.alert("可能是哪里出错了！");
                        }
                    },
                    error: function () {
                        layer.alert("服务器连接超时");
                    }
                })
            })
        })

    </script>
