            //Excel导出
            $("#EExport").click(function () {
                
                $.post("/GoodsInfo/ExcelINCGTest", {}, function (data) {
                    //if (data == "OK") {
                    //    layer.open({
                    //        title: '消息',
                    //        content: '数据导出成功!',
                    //        btn: ['确定'],
                    //        yes: function (index, layero) {
                    //        }
                    //    });
                    //} else

                    if (data == "Operation") {
                        layer.alert("您没有权限请和管理员联系");
                    }
                    else {
                        location.href = "@Url.Action("ExcelINCGTest", "GoodsInfo")";
                    }
                })
