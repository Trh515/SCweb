                OnePageC = $(this).val();
                SearchCGByGet(page, OnePageC)
            })

            //下一页
            $("#NextPage").click(function () {
                OnePageC = $("#pageList_btn").val();
                var pageNow = parseInt($("#pageNow").html());//当前页面
                if (pageNow < $("#Cpage").html()) {
                    page = pageNow + 1;
                    SearchCGByGet(page, OnePageC)//模糊查询的方法
                }
                else {
                    layer.alert("已经是最后一页!");
                }
            })

            //上一页
            $("#BackPage").click(function () {
                OnePageC = $("#pageList_btn").val();
                var pageNow = parseInt($("#pageNow").html());
                if (pageNow > 1) {
                    page = pageNow - 1;//当前页面
                    SearchCGByGet(page, OnePageC)//模糊查询的方法
                }
                else {
                    layer.alert("已经到第一页了!");
                }
