        <div class="sswin">

            <table class="sswin_table">
                <tr>
                    <td class="sswin_td1 sswin_koukuan"><span class="sspa">开始日期:</span></td>
                    <td class="sswin_td2">
                        <input type="text" name="kwd" class="ssinp ss_spjq " id="txtTime" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd',readOnly:true})">
                    </td>
                    <td class="sswin_td1 sswin_koukuan"><span class="sspa">结束日期:</span></td>
                    <td class="sswin_td2">
                        <input type="text" id="" class="ssinp" name="name">
                    </td>
                    <td class="sswin_td1 sswin_koukuan"><span class="sspa">业务类型:</span></td>
                    <td class="sswin_td2">
                        <input type="text" id="" class="ssinp" name="name">
                    </td>
                    <td class="sswin_td1 sswin_koukuan"><span class="sspa">供应商代码:</span></td>
                    <td class="sswin_td2">
