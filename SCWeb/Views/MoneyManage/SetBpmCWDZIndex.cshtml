                    <table class="nsw_pro_list" id="nsw_list_table" width="100%" style="text-align:center;">*@
                        <thead id="">
                            <tr id="tabHeader" class="">
                                <th class="p"><span class="cho"></span></th>
                                <th class="tcenter order" >开始日期</th>
                                <th class="tcenter order">结束日期</th>
                                <th class="order">业务类型</th>
                                <th class="order">供应商代码</th>
                                <th class="tcenter order">供应商全称</th>
                                <th class="tcenter order">期初金额</th>
                                <th class="tcenter order">入库\发货金额</th>
                                <th class="tcenter order">扣款金额</th>
                                <th class="tcenter order">付款金额</th>
                                <th class="tcenter order">期末金额</th>
