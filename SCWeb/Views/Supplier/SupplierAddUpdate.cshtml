                    //$("#GYSGC06").val('@ViewBag.GYSGC06');//绑定生产能力下拉
                    //$("#GYSGC07").val('@ViewBag.GYSGC07');//绑定质量管控能力下拉*@
                    $("#GYSGC11").val('@ViewBag.GYSGC11');//绑定是否与目标品牌合作下拉
                }
            })

            BindRadioQyC("@ViewBag.GYSGC28");//企业形态选中
            BindYwChk("@ViewBag.GYSGC30");//业务种类选中

