                    $("#GYSGC08").val('@ViewBag.GYSGC08');//绑定配合度下拉
                    $("#GYSGC09").val('@ViewBag.GYSGC09');//绑定价位下拉
                    $("#GYSGC12").val('@ViewBag.GYSGC12');//绑定采购部初步合作定位下拉
